{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": [
            "es2017",
            "es7",
            "es6",
            "dom"
        ],
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "removeComments": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
